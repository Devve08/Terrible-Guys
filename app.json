{
  "expo": {
    "name": "Terrible Guys",
    "slug": "productionApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icontwo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/1978s.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.terribleguys.terribleguys",
      "googleServicesFile": "./GoogleService-Info.plist"
     
    },
    "android": {
      "package": "com.devve08.testApp",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icontwo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      "@react-native-firebase/app"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8224721c-488d-4124-afa6-2d551c91b461"
      }
    }
  }
}
